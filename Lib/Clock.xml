<GuiXml>
    <Controls>
        <TopLevelControl name="Clock_TST_Time" movable="true" mouseEnabled="true" clampedToScreen="true" hidden="false">
            <Dimensions x="300" y="70" />
            <ClampedToScreenInsets top="20" bottom="-20" left="20" right="-20"/>
            <Anchor point="TOPLEFT" offsetX="100" offsetY="20"/>
            <Controls>
                <Texture name="$(parent)Background" textureFile="EsoUI/Art/Performance/StatusMeterMunge.dds">
                    <Dimensions x="600" y="256"/>
                    <Anchor point="CENTER" relativeTo="$(parent)" relativePoint="CENTER"/>
                </Texture>
                <Label name="$(parent)Label" font="ZoFontWinT2" color="INTERFACE_COLOR_TYPE_TEXT_COLORS:INTERFACE_TEXT_COLOR_NORMAL" horizontalAlignment="CENTER" verticalAlignment="CENTER" text="Fredas, 4th. Rain's Hand 2E 582 12:34:56 am">
                    <Anchor point="CENTER" relativeTo="$(parent)" relativePoint="CENTER"/>
                </Label>
            </Controls>

            <OnMouseEnter>
                Clock_TST_Time_OnMouseEnter(self)
            </OnMouseEnter>
            <OnMouseExit>
                Clock_TST_Time_OnMouseExit(self)
            </OnMouseExit>

            <OnMoveStop>
                Clock_TST_Time_OnMoveStop(self)
            </OnMoveStop>

            <OnInitialized>
                Clock_TST_Time_OnInitialized(self)
            </OnInitialized>
        </TopLevelControl>
        <TopLevelControl name="Clock_TST_Moon" movable="false" mouseEnabled="true" clampedToScreen="true">
            <Dimensions x="84" y="72" />
            <ClampedToScreenInsets top="20" bottom="-20" left="20" right="-20"/>
            <Anchor point="RIGHT" relativeTo="Clock_TST_Time" relativePoint="LEFT" offsetX="18" offsetY="8"/>
            <Controls>
                <Texture name="$(parent)Masser_Background" textureFile="Clock/Art/Moon/background_33.dds" hidden="false">
                    <Dimensions x="64" y="64"/>
                    <Anchor point="CENTER" relativeTo="$(parent)" relativePoint="CENTER"/>
                </Texture>
                <Texture name="$(parent)Masser" textureFile="Clock/dev/skygazer_moons/fadingsignal/textures/sky/masser_one_wax.dds">
                    <Dimensions x="64" y="64"/>
                    <Anchor point="CENTER" relativeTo="$(parent)Masser_Background" relativePoint="CENTER"/>
                </Texture>
                <Texture name="$(parent)Secunda_Background" textureFile="Clock/Art/Moon/background_33.dds" hidden="false">
                    <Dimensions x="40" y="40"/>
                    <Anchor point="TOP" relativeTo="$(parent)Masser" relativePoint="RIGHT"/>
                </Texture>
                <Texture name="$(parent)Secunda" textureFile="Clock/dev/skygazer_moons/fadingsignal/textures/sky/secunda_one_wax.dds">
                    <Dimensions x="40" y="40"/>
                    <Anchor point="CENTER" relativeTo="$(parent)Secunda_Background" relativePoint="CENTER"/>
                </Texture>
            </Controls>

            <OnMouseEnter>
                Clock_TST_Moon_OnMouseEnter(self)
            </OnMouseEnter>
            <OnMouseExit>
                Clock_TST_Moon_OnMouseExit(self)
            </OnMouseExit>

            <OnMoveStop>
                Clock_TST_Moon_OnMoveStop(self)
            </OnMoveStop>

            <OnInitialized>
                Clock_TST_Moon_OnInitialized(self)
            </OnInitialized>
        </TopLevelControl>
    </Controls>
</GuiXml>